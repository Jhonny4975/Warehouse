<div>
  <% if supplier.errors.any? %>
    <p>Verifique os erros abaixo:</p>
    <ul>
      <% supplier.errors.full_messages.each do |message| %>
      <li><%= message %></li>
      <% end %>
    </ul>
  <% end %>
</div>

<%= form_with model: supplier do |form| %>
  <div>
    <%= form.text_field :corporate_name, placeholder: 'Nome Fantasia:' %><br>
  </div>
  <div>
    <%= form.text_field :brand_name, placeholder: 'Razão Social:' %><br>
  </div>
  <div>
    <%= form.text_field :registration_number, placeholder: 'CNPJ:' %><br>
  </div>
  <div>
    <%= form.text_field :street_address, placeholder: 'Endereço:' %><br>
  </div>
  <div>
    <%= form.text_field :city, placeholder: 'Cidade:' %><br>
  </div>
  <div>
    <%= form.text_field :state, placeholder: 'Estado:' %><br>
  </div>
  <div>
    <%= form.text_field :email, placeholder: 'E-mail:' %><br>
  </div>
    <div>
    <%= form.text_field :phone_number, placeholder: 'Telefone:' %><br>
  </div>
  <div>
    <%= form.submit %>
  </div>
<% end %>
