<h1>Fornecedores</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Localização</th>
    </tr>
  </thead>
  <tbody>
    <% @suppliers.each do |supplier| %>
    <tr>
      <td><%= supplier.brand_name %></td>
      <td><%= supplier.city %> - <%= supplier.state %></td>
    </tr>
    <% end %>
  </tbody>
</table>
