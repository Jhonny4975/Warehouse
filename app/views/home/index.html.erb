<section id='sheds'>
  <h2>Galpões</h2>
  <% @sheds.each do |shed| %>
  <div>
    <h3><%= link_to shed.name, shed_path(shed.id) %></h3>
    <dl>
      <dt>Código: </dt>
      <dd><%= shed.code %></dd>
      <dt>Cidade: </dt>
      <dd><%= shed.city %></dd>
      <dt>Área: </dt>
      <dd><%= shed.area %> m²</dd>
    </dl>
    </div>
  <% end %>
  <div>
    <%= link_to 'Cadastrar galpão', new_shed_path %>
  </div>
</section>
